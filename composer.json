{
    "name": "ellaisys/telephony-exotel",
    "description": "Cloud Telephony Library - Exotel",
    "keywords": ["php", "laravel", "telephony", "cloud telephony", "cti", "exotel", "ellaisys"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/ellaisys/telephony-exotel",
    "authors": [
        {
            "name": "EllaiSys",
            "email": "support@ellaisys.com",
            "role": "owner",
            "homepage": "https://ellaisys.github.io/"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
          "Ellaisys\\Exotel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ellaisys\\Exotel\\Providers\\ExotelServiceProvider"
            ],
            "aliases": {
                "ExotelCall": "Ellaisys\\Exotel\\Facades\\ExotelCallFacade"
            }
        },
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
